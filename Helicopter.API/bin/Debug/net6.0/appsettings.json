{
    "SwaggerOptions": {
      "JsonRoute": "swagger/{documentName}/swagger.json",
      "Description": "Helicoptro",
      "UIEndpoint": "v1/swagger.json"
    },
    "RedisOptions": {
      "Enabled": true,
      "ConnectionString": "localhost"
    },
    "ConnectionStrings": {
      "Default": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=Helicopter;Integrated Security=True;Connect Timeout=30;Encrypt=False;TrustServerCertificate=False;ApplicationIntent=ReadWrite;MultiSubnetFailover=False"
    },

    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "AllowedHosts": "*"
}
